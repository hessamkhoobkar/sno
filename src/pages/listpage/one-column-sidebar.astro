---
import MainLayout from "@layouts/MainLayout.astro";
import OneColWithSidebar from "@components/list-layout/OneColWithSidebar.astro";

// Get data from Astro's content collection
import { getCollection } from "astro:content";
const allBlogPost = await getCollection("blog");
---

<MainLayout>
  <OneColWithSidebar postData={allBlogPost} />
</MainLayout>
